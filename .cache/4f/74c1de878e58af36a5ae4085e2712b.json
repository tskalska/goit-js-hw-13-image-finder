form-container {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.search-form {\r\n  margin-right: 40px;\r\n}\r\n\r\n.view-more-button {\r\n  visibility:hidden;\r\n  display: block;\r\n  margin: auto;\r\n}\r\n\r\n.input__search-form{\r\n  font-size: 16px;\r\n  line-height: 28px;\r\n  padding: 8px 16px;\r\n  width: 100%;\r\n  min-height: 44px;\r\n\r\n  border: unset;\r\n  border-radius: 4px;\r\n  outline-color: #8fcedd;\r\n  background-color: rgb(255, 255, 255);\r\n  box-shadow: rgba(0, 0, 0, 0) 0px 0px 0px 0px, \r\n              rgba(0, 0, 0, 0) 0px 0px 0px 0px, \r\n              rgba(0, 0, 0, 0) 0px 0px 0px 0px, \r\n              rgba(60, 66, 87, 0.16) 0px 0px 0px 1px, \r\n              rgba(0, 0, 0, 0) 0px 0px 0px 0px, \r\n              rgba(0, 0, 0, 0) 0px 0px 0px 0px, \r\n              rgba(0, 0, 0, 0) 0px 0px 0px 0px;\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"4b82a8c750f2737c9cdeab69382931fa","cacheData":{"env":{}}}